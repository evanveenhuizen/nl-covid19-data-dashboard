{
  "name": "@corona-dashboard/common",
  "private": true,
  "version": "0.0.0",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "echo 'There are no tests for packages/common yet'",
    "test:ci": "echo 'There are no tests for packages/common yet.'",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "clean": "del-cli dist tsconfig.tsbuildinfo",
    "compile": "tsc -b .",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "format": "prettier --write \"src/**/*.{ts,tsx,md,json,yml}\" \"*.{js,json,md,yml}\"",
    "rollup": "rollup -c rollup.config.ts"
  },
  "dependencies": {
    "lodash-es": "^4.17.20",
    "ts-is-present": "^1.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "@types/jest": "^26.0.14",
    "@types/lodash-es": "^4.17.4",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.1",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "rollup": "^2.38.5",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript-paths": "^1.2.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.5"
  }
}
